    chmod 0664 /sys/class/input_booster/head
    chmod 0664 /sys/class/input_booster/tail

    chown system log /proc/sec_log

    chown bluetooth bluetooth /dev/ttySAC3
    chmod 0660 /dev/ttySAC3

# permissions for bluetooth.
